<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.ione.project.AvtosalonEntity" table="Avtosalon" schema="Lab_5_b_Hibernate">
        <id name="idAvtosalon">
            <column name="idAvtosalon" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="ownerName">
            <column name="ownerName" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>